<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_search"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.project_myfit.search.SearchFragment"
        tools:layout="@layout/fragment_search"
        android:label="SearchFragment">
        <action
            android:id="@+id/action_searchFragment_to_searchSelectedItemDeleteDialog"
            app:destination="@id/searchSelectedItemDeleteDialog" />
        <action
            android:id="@+id/action_searchFragment_to_searchTreeViewDialog"
            app:destination="@id/searchTreeViewDialog" />
        <action
            android:id="@+id/action_searchFragment_to_searchNameEditDialog"
            app:destination="@id/searchNameEditDialog" />
        <action
            android:id="@+id/action_searchFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_searchFragment_to_recentSearchDeleteAllDialog"
            app:destination="@id/recentSearchDeleteAllDialog" />
    </fragment>
    <dialog
        android:id="@+id/searchSelectedItemDeleteDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.SearchSelectedItemDeleteDialog"
        android:label="SearchSelectedItemDeleteDialog">
        <argument
            android:name="selectedItemSize"
            app:argType="integer" />
    </dialog>
    <dialog
        android:id="@+id/searchTreeViewDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.SearchTreeViewDialog"
        android:label="SearchTreeViewDialog">
        <argument
            android:name="parentCategory"
            app:argType="string" />
        <action
            android:id="@+id/action_searchTreeViewDialog_to_searchAddDialog"
            app:destination="@id/searchAddDialog" />
        <action
            android:id="@+id/action_searchTreeViewDialog_to_searchItemMoveDialog"
            app:destination="@id/searchItemMoveDialog" />
    </dialog>
    <dialog
        android:id="@+id/searchAddDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.SearchAddDialog"
        android:label="SearchAddDialog">
        <argument
            android:name="itemType"
            app:argType="string" />
        <argument
            android:name="parentCategory"
            app:argType="string" />
        <argument
            android:name="parentId"
            app:argType="long" />
        <action
            android:id="@+id/action_searchAddDialog_to_searchSameNameAddDialog"
            app:destination="@id/searchSameNameAddDialog" />
    </dialog>
    <dialog
        android:id="@+id/searchItemMoveDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.SearchItemMoveDialog"
        android:label="SearchItemMoveDialog">
        <argument
            android:name="selectedItemSize"
            app:argType="integer" />
        <argument
            android:name="parentId"
            app:argType="long" />
    </dialog>
    <dialog
        android:id="@+id/searchSameNameAddDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.SearchSameNameAddDialog"
        android:label="SearchSameNameAddDialog">
        <argument
            android:name="itemType"
            app:argType="string" />
        <argument
            android:name="parentCategory"
            app:argType="string" />
        <argument
            android:name="parentId"
            app:argType="long" />
        <argument
            android:name="newName"
            app:argType="string" />
    </dialog>
    <dialog
        android:id="@+id/searchNameEditDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.SearchNameEditDialog"
        android:label="SearchNameEditDialog">
        <argument
            android:name="folderId"
            app:argType="long" />
        <action
            android:id="@+id/action_searchNameEditDialog_to_searchSameNameEditDialog"
            app:destination="@id/searchSameNameEditDialog" />
    </dialog>
    <dialog
        android:id="@+id/searchSameNameEditDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.SearchSameNameEditDialog"
        android:label="SearchSameNameEditDialog">
        <argument
            android:name="folderId"
            app:argType="long" />
        <argument
            android:name="newName"
            app:argType="string" />
    </dialog>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.project_myfit.MainActivity"
        android:label="MainActivity">
        <argument
            android:name="folderId"
            app:argType="long" />
        <argument
            android:name="sizeId"
            app:argType="long" />
        <argument
            android:name="parentId"
            app:argType="long" />
    </activity>
    <dialog
        android:id="@+id/recentSearchDeleteAllDialog"
        android:name="com.example.project_myfit.dialog.searchdialog.RecentSearchDeleteAllDialog"
        android:label="RecentSearchDeleteAllDialog" />
</navigation>