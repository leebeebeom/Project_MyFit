<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_search"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.project_myfit.search.SearchFragment"
        tools:layout="@layout/fragment_search"
        android:label="SearchFragment">
        <action
            android:id="@+id/toMainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/toDeleteAllRecentSearchDialog"
            app:destination="@id/deleteAllRecentSearchDialog" />
        <action
            android:id="@+id/toTreeViewDialog"
            app:destination="@id/treeViewDialog" />
        <action
            android:id="@+id/toEditNameDialog"
            app:destination="@id/editNameDialog" />
        <action
            android:id="@+id/toDeleteSelectedItemDialog"
            app:destination="@id/deleteSelectedItemDialog" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.project_myfit.MainActivity"
        android:label="MainActivity">
        <argument
            android:name="folderId"
            app:argType="long" />
        <argument
            android:name="sizeId"
            app:argType="long" />
        <argument
            android:name="parentId"
            app:argType="long" />
    </activity>
    <dialog
        android:id="@+id/deleteAllRecentSearchDialog"
        android:name="com.example.project_myfit.dialog.DeleteAllRecentSearchDialog"
        android:label="DeleteAllRecentSearchDialog">
        <argument
            android:name="navGraphId"
            app:argType="integer" />
    </dialog>
    <dialog
        android:id="@+id/addDialog"
        android:name="com.example.project_myfit.dialog.AddDialog"
        android:label="AddDialog">
        <argument
            android:name="itemType"
            app:argType="integer" />
        <argument
            android:name="parentCategory"
            app:argType="string" />
        <argument
            android:name="parentId"
            app:argType="long" />
        <argument
            android:name="navGraphId"
            app:argType="integer" />
        <action
            android:id="@+id/toAddSameNameDialog"
            app:destination="@id/addSameNameDialog" />
    </dialog>
    <dialog
        android:id="@+id/addSameNameDialog"
        android:name="com.example.project_myfit.dialog.AddSameNameDialog"
        android:label="AddSameNameDialog">
        <argument
            android:name="itemType"
            app:argType="integer" />
        <argument
            android:name="parentCategory"
            app:argType="string" />
        <argument
            android:name="parentId"
            app:argType="long" />
        <argument
            android:name="name"
            app:argType="string" />
        <argument
            android:name="navGraphId"
            app:argType="integer" />
    </dialog>
    <dialog
        android:id="@+id/treeViewDialog"
        android:name="com.example.project_myfit.dialog.TreeViewDialog"
        android:label="TreeViewDialog">
        <argument
            android:name="parentCategory"
            app:argType="string" />
        <argument
            android:name="thisCategoryId"
            app:argType="long" />
        <argument
            android:name="thisFolderId"
            app:argType="long" />
        <argument
            android:name="navGraphId"
            app:argType="integer" />
        <action
            android:id="@+id/toAddDialog"
            app:destination="@id/addDialog" />
        <action
            android:id="@+id/toMoveDialog"
            app:destination="@id/moveDialog" />
    </dialog>
    <dialog
        android:id="@+id/moveDialog"
        android:name="com.example.project_myfit.dialog.MoveDialog"
        android:label="MoveDialog">
        <argument
            android:name="selectedItemSize"
            app:argType="integer" />
        <argument
            android:name="parentId"
            app:argType="long" />
        <argument
            android:name="navGraphId"
            app:argType="integer" />
    </dialog>
    <dialog
        android:id="@+id/editNameDialog"
        android:name="com.example.project_myfit.dialog.EditNameDialog"
        android:label="toEditNameDialog">
        <argument
            android:name="itemId"
            app:argType="long" />
        <argument
            android:name="itemType"
            app:argType="integer" />
        <argument
            android:name="isParentName"
            app:argType="boolean" />
        <argument
            android:name="navGraphId"
            app:argType="integer" />
        <action
            android:id="@+id/toEditSameNameDialog"
            app:destination="@id/editSameNameDialog" />
    </dialog>
    <dialog
        android:id="@+id/editSameNameDialog"
        android:name="com.example.project_myfit.dialog.EditSameNameDialog"
        android:label="EditSameNameDialog">
        <argument
            android:name="itemType"
            app:argType="integer" />
        <argument
            android:name="itemId"
            app:argType="long" />
        <argument
            android:name="newName"
            app:argType="string" />
        <argument
            android:name="isParentName"
            app:argType="boolean" />
        <argument
            android:name="navGraphId"
            app:argType="integer" />
    </dialog>
    <dialog
        android:id="@+id/deleteSelectedItemDialog"
        android:name="com.example.project_myfit.dialog.DeleteSelectedItemDialog"
        android:label="DeleteSelectedItemDialog">
        <argument
            android:name="selectedItemSize"
            app:argType="integer" />
        <argument
            android:name="navGraphId"
            app:argType="integer" />
    </dialog>
</navigation>